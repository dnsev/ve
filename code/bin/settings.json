{
	"app": {
		"local_look": true,
		"update_check": true,
		"temp_dir": "temp",
		"show_advanced_tab": false,
		"extensions": {
			"image": [ ".png" , ".jpg" , ".jpeg" , ".gif" ],
			"audio": [ ".mp3" , ".ogg" , ".oga" , ".flac" , ".wav" ],
			"video": [ ".mkv" , ".webm" , ".mp4" , ".divx" , ".xvid" , ".avi" , ".3gp" , ".wmv" , ".flv" , ".f4v" , ".mpg" , ".mpeg" , ".ts" ],
			"output": [ ".ve*" , ".ev*" , "*" ],
			"output_default": ".ve*"
		}
	},
	"gui": {
		"color_matrix": [
			1 , 0 , 0 ,
			0 , 1 , 0 ,
			0 , 0 , 1
		],
		"color_scale": [
			1 , 1 , 1
		],
		"default_tab": "Video"
	},
	"settings": {
		"output_max_file_size": 3072000,
		"video_encoding_mode": 1,
		"video_auto_quality_on": true,
		"video_auto_quality": "Average",
		"image_quality": 1,
		"audio_kbps": 80,
		"audio_channels": -1,
		"audio_sample_rate": -1
	},
	"ffmpeg": {
		"statup_check": true,
		"threads": -1
	}
}